#include <stdio.h>
    int main() {    
        int a,b,s,m,sub;
        float d;
        
        printf("Enter two integers: ");
        scanf("%d %d", &a, &b);
        s=a+b;
        printf(" Addition of two numbers is= %d \n",s);
        d=a/(float)b;
        printf(" Division of two numbers is= %f \n",d);
        m=a*b;
        printf(" Multiplication of two numbers is=%d \n",m);
        sub=a-b;
        printf(" Subtraction of two numbers is=%d \n",sub);
        
                return 0;
    }
